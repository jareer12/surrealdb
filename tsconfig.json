{
  "compilerOptions": {
    "target": "ES6",
    "module": "Node16",
    "strict": true,
    "sourceMap": true,
    "declaration": true,
    "declarationMap": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "esModuleInterop": true,
    "outDir": "./lib",
  },
